{
    "name": "nuvem-painel-adm",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "author": "author",
    "license": "ISC",
    "scripts": {
        "link": "pm2 link icso8t3fbkpqttx gk3fl9nw7fhhsd2",
        "copy": "cp -r -u ./src/main/pb/* ./deploy/main/pb",
        "dev": "npm-run-all -p dev:*",
        "dev:server": "nodemon src/main/server.js",
        "install:pm2": "npm install pm2 -g",
        "start": "npm install && npm run deploy && npm run copy && npm run link &&  npm run prod:server && pm2 log",
        "prod:server": "pm2 start deploy/main/server.js --name ADMIN-TOKEN",
        "reload": "pm2 reload server",
        "deploy": "sucrase ./src -d ./deploy --transforms imports",
        "migration": "yarn sequelize migration:create",
        "migration:undo": "yarn sequelize db:migrate:undo",
        "migrate": "yarn sequelize db:migrate",
        "test:cov": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandles -- --coverage -c jest-unit.config.js",
        "test:unit": "yarn jest --passWithNoTests --runInBand --detectOpenHandles -c jest-unit.config.js",
        "test:staged": "yarn jest --passWithNoTests --silent --runInBand --detectOpenHandle --findRelatedTests --silent -c jest-unit.config.js",
        "pretest:integration": "cross-env NODE_ENV=test yarn migrate",
        "test:integration": "cross-env NODE_ENV=test yarn jest --watch -c jest-integration-config.js --detectOpenHandles",
        "posttest:integration": "cross-env NODE_ENV=test yarn migration:undo",
        "pretest:repository": "cross-env NODE_ENV=test yarn migrate",
        "test:repository": "cross-env NODE_ENV=test yarn jest --watch -c jest-repository-config.js --detectOpenHandles",
        "posttest:repository": "cross-env NODE_ENV=test yarn migration:undo"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@grpc/grpc-js": "^1.3.3",
        "@grpc/proto-loader": "^0.5.6",
        "axios": "^0.25.0",
        "dd-trace": "^2.9.1",
        "dotenv": "^8.2.0",
        "jsonwebtoken": "^8.5.1",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "pm2": "^4.5.4",
        "sucrase": "^3.17.0",
        "supertest": "^6.1.3"
    },
    "devDependencies": {
        "@jest-mock/express": "^1.3.0",
        "@sucrase/jest-plugin": "^2.0.0",
        "@types/jest": "^26.0.20",
        "babel-jest": "^26.6.3",
        "cross-env": "^7.0.3",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-prettier-config": "^1.0.1",
        "git-commit-msg-linter": "^3.0.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "sinon": "^9.2.4",
        "ts-jest": "^26.4.4"
    }
}
